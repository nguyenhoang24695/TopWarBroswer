<Window x:Class="AutoBroswer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoBroswer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="TestOpenApp" HorizontalAlignment="Left" Margin="25,28,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Recolize" HorizontalAlignment="Left" Margin="25,71,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Click" HorizontalAlignment="Left" Margin="25,107,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <TextBox x:Name="toa_do_x" HorizontalAlignment="Left" Height="23" Margin="133,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="49"/>
        <TextBox x:Name="toa_do_y" HorizontalAlignment="Left" Height="23" Margin="204,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="51"/>

    </Grid>
</Window>
